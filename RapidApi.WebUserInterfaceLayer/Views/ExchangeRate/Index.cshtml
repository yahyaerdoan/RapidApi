@model List<RapidApi.WebUserInterfaceLayer.Models.MovieViewModels.ExchangeRatesViewModel.Exchange_Rates>

@{
    Layout = null;
}
<link href="~/lib/bootstrap/dist/css/bootstrap.min.css" rel="stylesheet" />

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <div class="container-fluid">
        <table class="table table-hover">
            <thead>
                <tr>
                    <th scope="col">#</th>
                    <th scope="col">Name</th>
                    <th scope="col">Description</th>
                    <th scope="col">See on web</th>
                </tr>
            </thead>
            <tbody>
                @foreach (var item in Model)
                {
                    <tr>
                        <td>@item.currency</td>
                        <th scope="row">@item.exchange_rate_buy</th> 
                    </tr>
                }
            </tbody>
        </table>
    </div>
</body>
</html>
